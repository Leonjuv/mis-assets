{% doc %}
  @prompt
    Create a section where you can edit gradient background colors and curved borders, where you can add tabs, edit tab titles and content., 1. Remove the gradient background.

    2. Add a function to adjust the section width., 1. The background color isn't applying correctly; please fix it.

    2. The section width isn't working; please fix it.
{% enddoc %}
{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-tabs-section-wrapper-{{ ai_gen_id }} {
    width: 100%;
    padding: 0 20px;
  }

  .ai-tabs-section-{{ ai_gen_id }} {
    padding: {{ block.settings.section_padding_top }}px 30px {{ block.settings.section_padding_bottom }}px;
    background-color: {{ block.settings.background_color }};
    border-radius: {{ block.settings.border_radius }}px;
    width: 100%;
    max-width: {{ block.settings.section_width }}px;
    margin: 0 auto;
  }

  .ai-tabs-container-{{ ai_gen_id }} {
    width: 100%;
  }

  .ai-tabs-header-{{ ai_gen_id }} {
    text-align: {{ block.settings.heading_alignment }};
    margin-bottom: 30px;
  }

  .ai-tabs-title-{{ ai_gen_id }} {
    font-size: {{ block.settings.heading_size }}px;
    color: {{ block.settings.heading_color }};
    margin: 0 0 10px;
    font-weight: 700;
  }

  .ai-tabs-subtitle-{{ ai_gen_id }} {
    font-size: {{ block.settings.subheading_size }}px;
    color: {{ block.settings.subheading_color }};
    margin: 0;
  }

  .ai-tabs-nav-{{ ai_gen_id }} {
    display: flex;
    gap: 10px;
    margin-bottom: 30px;
    flex-wrap: wrap;
    justify-content: {{ block.settings.tabs_alignment }};
  }

  .ai-tab-button-{{ ai_gen_id }} {
    padding: 12px 24px;
    background-color: {{ block.settings.tab_bg_color }};
    color: {{ block.settings.tab_text_color }};
    border: {{ block.settings.tab_border_width }}px solid {{ block.settings.tab_border_color }};
    border-radius: {{ block.settings.tab_border_radius }}px;
    cursor: pointer;
    font-size: {{ block.settings.tab_font_size }}px;
    font-weight: 600;
    transition: all 0.3s ease;
  }

  .ai-tab-button-{{ ai_gen_id }}:hover {
    background-color: {{ block.settings.tab_hover_bg_color }};
    color: {{ block.settings.tab_hover_text_color }};
  }

  .ai-tab-button-{{ ai_gen_id }}.active {
    background-color: {{ block.settings.tab_active_bg_color }};
    color: {{ block.settings.tab_active_text_color }};
    border-color: {{ block.settings.tab_active_border_color }};
  }

  .ai-tabs-content-{{ ai_gen_id }} {
    position: relative;
  }

  .ai-tab-panel-{{ ai_gen_id }} {
    display: none;
    padding: {{ block.settings.content_padding }}px;
    background-color: {{ block.settings.content_bg_color }};
    border-radius: {{ block.settings.content_border_radius }}px;
    border: {{ block.settings.content_border_width }}px solid {{ block.settings.content_border_color }};
    color: {{ block.settings.content_text_color }};
    animation: ai-tab-fade-in-{{ ai_gen_id }} 0.3s ease;
  }

  .ai-tab-panel-{{ ai_gen_id }}.active {
    display: block;
  }

  .ai-tab-panel-title-{{ ai_gen_id }} {
    font-size: {{ block.settings.content_title_size }}px;
    color: {{ block.settings.content_title_color }};
    margin: 0 0 15px;
    font-weight: 600;
  }

  .ai-tab-panel-content-{{ ai_gen_id }} {
    font-size: {{ block.settings.content_font_size }}px;
    line-height: 1.6;
  }

  @keyframes ai-tab-fade-in-{{ ai_gen_id }} {
    from {
      opacity: 0;
      transform: translateY(10px);
    }
    to {
      opacity: 1;
      transform: translateY(0);
    }
  }

  @media screen and (max-width: 749px) {
    .ai-tabs-section-wrapper-{{ ai_gen_id }} {
      padding: 0 15px;
    }

    .ai-tabs-section-{{ ai_gen_id }} {
      padding: {{ block.settings.section_padding_top | times: 0.6 }}px 20px {{ block.settings.section_padding_bottom | times: 0.6 }}px;
    }

    .ai-tabs-nav-{{ ai_gen_id }} {
      justify-content: center;
    }

    .ai-tab-button-{{ ai_gen_id }} {
      font-size: {{ block.settings.tab_font_size | times: 0.9 }}px;
      padding: 10px 18px;
    }

    .ai-tabs-title-{{ ai_gen_id }} {
      font-size: {{ block.settings.heading_size | times: 0.8 }}px;
    }

    .ai-tab-panel-{{ ai_gen_id }} {
      padding: {{ block.settings.content_padding | times: 0.7 }}px;
    }
  }
